{
    "name": "nexusmerchants/infusionsoft-php",
    "description": "Infusionsoft PHP SDK",
    "keywords": [
        "infusionsoft",
        "sdk"
    ],
    "homepage": "https://developer.infusionsoft.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Infusionsoft",
            "homepage": "https://developer.infusionsoft.com"
        },
        {
            "name": "Peter Harlacher",
            "email": "peter@nexusmerchants.com",
            "homepage": "https://www.nexusmerchants.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "lstrojny/fxmlrpc": "^0.15.0",
        "psr/log": "~1.0",
        "php-http/guzzle7-adapter": "^0.1",
        "laminas/laminas-diactoros": "^2.4",
        "php-http/message": "^1.6",
        "php-http/curl-client": "^2.1"
    },
    "require-dev": {
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "~4",
        "doctrine/instantiator": "1.0.5",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Infusionsoft": ["src", "tests"]
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infusionsoft\\FrameworkSupport\\Laravel\\InfusionsoftServiceProvider"
            ]
        }
    }
}
